{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "awcodes/filament-tiptap-editor": "^3.0",
        "awcodes/light-switch": "^1.0",
        "barryvdh/laravel-dompdf": "^2.0",
        "bezhansalleh/filament-shield": "^3.3",
        "binarytorch/larecipe": "^2.6",
        "cheesegrits/filament-google-maps": "^3.0",
        "coolsam/signature-pad": "^2.0",
        "filament/filament": "^3.2",
        "filament/notifications": "^3.2",
        "filament/spatie-laravel-settings-plugin": "^3.2",
        "filament/spatie-laravel-translatable-plugin": "^3.2",
        "filament/tables": "^3.2",
        "guzzlehttp/guzzle": "^7.2",
        "icetalker/filament-table-repeater": "^1.1",
        "laravel/framework": "^11.00",
        "laravel/pulse": "^1.2",
        "laravel/sanctum": "^4.0",
        "laravel/tinker": "^2.9",
        "leandrocfe/filament-apex-charts": "^3.1",
        "livewire/livewire": "^3.5",
        "maatwebsite/excel": "^3.1",
        "marjose123/filament-lockscreen": "^2.0",
        "njxqlus/filament-progressbar": "^1.0",
        "psr/simple-cache": "^2.0",
        "pxlrbt/filament-excel": "^2.3",
        "pxlrbt/filament-spotlight": "^1.3",
        "stechstudio/filament-impersonate": "^3.6",
        "tomatophp/filament-logger": "^1.0",
        "tomatophp/filament-media-manager": "^1.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
